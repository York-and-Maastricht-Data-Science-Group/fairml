operation Collection listToLine(): String {
	return self
		.collect(item | "'" + item + "'")
		.concat(", ");
}

operation Collection listToLineWithoutQuote(): String {
	return self
		.collect(item | item )
		.concat(", ");
}

operation FairML inc(value: Integer): Integer {
	value := value + 1;
	return value;
		
}
